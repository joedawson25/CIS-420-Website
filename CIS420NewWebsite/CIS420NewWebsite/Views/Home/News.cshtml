@model IEnumerable<CIS420NewWebsite.Models.Article>
@{
    ViewBag.Title = "News";

}
<html>

<h2>National News</h2>
<div style="width: 100%;">
    <div style="float:left; width: 50%;">
        <center><img alt="First Picture" src="~/Content/Images/Horses_Go.jpg" style="width:800px;height:400px;"></center>
        </div>
    <div style="float:right; width: 50%;">
        @foreach (var article in Model)
            {
            <li>
                <a target="_blank" href="@article.Link">
                    @article.Title<br />
                    @article.PublicationDate<br />
                </a>
            </li>
        }
        </ul>
    </div>
</div>
<div style="clear:both"></div>

    

</html>

